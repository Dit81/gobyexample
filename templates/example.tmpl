  <body>
    <div class="example" id="{{.Id}}">
      <h2><a href="./">Go в примерах</a>: {{.Title}}</h2>
      {{range .Segs}}
      <table>
        {{range .}}
        <tr>
          <td class="docs">
            {{.DocsRendered}}
          </td>
          <td class="code{{if .CodeEmpty}} empty{{end}}{{if .CodeLeading}} leading{{end}}">
	        {{if .CodeRun}}<a href="http://play.golang.org/p/{{$.UrlHash}}"><img title="Запустить код" src="play.png" class="run" /></a>{{end}}
            {{.CodeRendered}}
          </td>
        </tr>
        {{end}}
      </table>
      {{end}}
      {{if .NextExample}}
      <p class="next">
        Следующий пример: <a href="{{.NextExample.Id}}.html">{{.NextExample.Title}}</a>.
      </p>
      {{end}}
      <p class="footer">
        придумал <a href="https://twitter.com/mmcgrana">@mmcgrana</a> | перевел <a href="http://twitter.com/village_geek">@village_geek</a> | <a href="https://github.com/pavlik/gobyexample/blob/master/examples/{{.Id}}">исходники</a> | <a href="https://github.com/pavlik/gobyexample#license">лицензия</a>
      </p>
    </div>
  </body>
